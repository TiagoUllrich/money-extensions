<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>disable</Nullable>

    <PackageId>MoneyExtensions_TFU</PackageId>
    <Version>1.0.0</Version>    
    <License>MIT</License>
    <Authors>Tiago Ullrich</Authors>    
    <PackageDescription>Extension to convert decimal to integer</PackageDescription>
    <Description>Extension to convert decimal to integer</Description>
    <RepositoryUrl>https://github.com/TiagoUllrich/money-extensions</RepositoryUrl>
    <PackageProjectUrl>https://github.com/TiagoUllrich/money-extensions</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>Icon.png</PackageIcon>
    <PackageTags>money;int;decimal;converter</PackageTags>    
    <Title>MoneyExtensions</Title>
    <Summary>Extension to convert decimal to integer.</Summary>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Media\Icon.png" Pack="true" PackagePath="\"/>
  </ItemGroup>

